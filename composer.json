{
    "name": "Tanswdc/laravel-calendar",
    "description": "Laravel helper for FullCalendar.io forked from acaronlex/laravel-calendar to install on Laravel 9",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "laravel-calendar",
            "email": "acaronlex@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "illuminate/support": "^8.75 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tanswdc\\LaravelCalendar\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanswdc\\LaravelCalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Tanswdc\\LaravelCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
