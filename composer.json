{
    "name": "donions/laravel-calendar",
    "description": "Laravel helper for FullCalendar.io forked from acaronlex/laravel-calendar to install on Laravel 9",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "laravel-calendar",
            "email": "acaronlex@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dhonions\\LaravelCalendar\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dhonions\\LaravelCalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Dhonions\\LaravelCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
